{
  "name": "react-boilerplate-vscode",
  "version": "1.0.0",
  "description": "Boilerplate for React w/Redux and VSCode support",
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "live",
    "edit",
    "webpack",
    "babel"
  ],
  "main": "index.js",
  "repository": "git@github.com:DavidKindler/React-Boilerplate-VSCode.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test/components",
    "test:watch": "npm run test -- --watch",
    "watch": "webpack --watch ",
    "build": "webpack --config webpack.config.prod.js",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "David Kindler",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "html-webpack-plugin": "^2.28.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": ">=3.4.0",
    "mocha": "^2.4.5",
    "webpack": "^2.3.3",
    "webpack-dev-server": ">=3.1.11",
    "jsdom": "^8.1.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.17",
    "react": "^15.5.0",
    "react-redux": "^5.1.2",
    "react-dom": "^16.0.1",
    "react-router": "^4.3.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "snyk": "^1.337.0"
  },
  "snyk": true
}
